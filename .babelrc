{
	"presets": [
		"env",
		"react"
	],
	"plugins": [
		"transform-runtime",
		"babel-plugin-transform-object-rest-spread",
		"babel-plugin-styled-components",
		[
			"module-resolver",
			{
				"root": [
					"./client"
				],
				"alias": {
					"components": "./client/components",
					"utils": "./client/utils"
				}
			}
		]
	]
}
{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-transform-modules-commonjs",
    "babel-plugin-styled-components",
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./client/components",
          "client": "./client",
          "server": "./server",
          "tests": "./__tests__"
        }
      }
    ]
  ]
}
